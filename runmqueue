gcc -g -o bin/mr src/processes/mr_process.c src/ipcs/mqueue.c src/utils/ipcs_protocol.c src/utils/process_utils.c -lpthread
gcc -g -o bin/ml src/processes/ml_process.c src/ipcs/mqueue.c src/utils/ipcs_protocol.c src/utils/process_utils.c -lpthread
